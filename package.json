{
  "name": "openclaw-backup",
  "version": "0.1.0",
  "description": "Backup and restore plugin for OpenClaw \u2014 multi-provider snapshots with encryption",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openclaw-backup": "dist/restore/standalone.js"
  },
  "scripts": {
    "build": "tsc && cp openclaw.plugin.json dist/",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"*.md\" \"*.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.md\" \"*.json\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "quality": "npm run build && npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.7.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist/",
    "openclaw.plugin.json",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "openclaw",
    "backup",
    "restore",
    "plugin"
  ],
  "author": "Brian Colinger",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/briancolinger/openclaw-backup-plugin.git"
  },
  "openclaw": {
    "extensions": [
      "dist/index.js"
    ]
  }
}